package Baekjoon;

import java.util.Scanner;

public class test3 {

	public static void main(String[] args) {

	Scanner sc = new Scanner(System.in);
	
		//2739
//	int n = sc.nextInt();
	for(int i = 2; i <= 9; i++) {
		for(int j = 1; j <= 9; j++) {
			int y= i*j;
			System.out.println(i+ "x" + j + "=" + y);
		}
	}
	}

}
