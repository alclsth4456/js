package Baekjoon;

import java.util.Scanner;

public class test3 {

	public static void main(String[] args) {

	Scanner sc = new Scanner(System.in);
	
		//2739
	int n = sc.nextInt();
	for(int i = n; i <= (n-1); i++) {
		for(int j = 1; j <=9; j++) {
			int y= n*j;
			System.out.println(n+ "x" + j + "=" + y);
		}
	}
	}

}
